{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aulaisa"
		},
		"AzureDataLakeStorage1aulaisa_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1aulaisa'"
		},
		"AzureMySql1SakilaAulaIsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySql1SakilaAulaIsa'"
		},
		"MySql1aulaisa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySql1aulaisa'"
		},
		"AzureDataLakeStorage1aulaisa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://aula201221.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aulaisa_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureMySql1SakilaAulaIsa_connectionString": {
						"value": "[parameters('AzureMySql1SakilaAulaIsa_connectionString')]"
					},
					"AzureDataLakeStorage1aulaisa_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1aulaisa_accountKey')]"
					},
					"MySql1aulaisa_connectionString": {
						"value": "[parameters('MySql1aulaisa_connectionString')]"
					},
					"AzureDataLakeStorage1aulaisa_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1aulaisa_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}